{
  "name": "json-custom-view",
  "version": "1.2.0",
  "description": "The JavaScript (frontend module) which process JSON data and render it on a web page with various settings.",
  "main": "build/api.js",
  "types": "build/api.d.ts",
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/node_modules/jest-css-modules"
    }
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve",
    "build-dev": "cross-env NODE_ENV=development webpack",
    "build": "cross-env NODE_ENV=production webpack && cp src/api.d.ts build",
    "copy-types": "cp src/api.d.ts build",
    "full-build": "npm run build && npm run copy-types",
    "editorconfig": "editorconfig-checker",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/polina-prokofieva/view-json.git#main"
  },
  "keywords": [
    "javascript",
    "typescript",
    "frontend",
    "json",
    "data"
  ],
  "author": "Polina Prokofieva",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/polina-prokofieva/view-json/issues"
  },
  "homepage": "https://github.com/polina-prokofieva/view-json/tree/main#readme",
  "browserslist": [
    "last 2 chrome versions",
    "last 2 firefox versions",
    "last 1 edge versions"
  ],
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.20.0",
    "@jest/globals": "^29.4.3",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "editorconfig-checker": "^5.0.1",
    "eslint": "^9.20.0",
    "expect": "^29.3.1",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.3.1",
    "jest-cli": "^29.3.1",
    "jest-css-modules": "^2.1.0",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  }
}
